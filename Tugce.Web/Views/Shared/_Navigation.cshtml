
<section id="inner-headline">
    <div class="container">
        <div class="row">
            <div class="span4">
                <div class="inner-heading">
                    <h2>@ViewBag.Title</h2>
                </div>
            </div>
            <div class="span8">
                <ul class="breadcrumb">
                    <li><a href="@Url.Action("Index","Home")"><i class="icon-home"></i></a><i class="icon-angle-right"></i></li>
                    <li><a href="@(Request.Cookies["prev"]!=null?(Request.Cookies["prev"] as HttpCookie).Value:"#")">Önceki Sayfa</a><i class="icon-angle-right"></i></li>
                    <li class="active">@ViewBag.Title</li>
                </ul>
            </div>
        </div>
    </div>
</section>
